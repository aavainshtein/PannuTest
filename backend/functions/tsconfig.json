{
  "compilerOptions": {
    "allowJs": true,
    "skipLibCheck": true,
    "noEmit": true,
    "module": "commonjs",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "isolatedModules": true,
    "strictNullChecks": false,
    "target": "ES6", // или "ES2015" и выше
    "lib": ["ES6", "DOM"], // Добавьте "DOM", если работа с браузерными API
  }
}
